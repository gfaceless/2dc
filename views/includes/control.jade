if logged_in
  ul#control
    if mid
      li: a(href="/products/register") 上传/注册产品
      li: a(href="/products?mfr=#{locals.mid}") 我的所有产品
      li: a(href="/mfrs/#{locals.mid}") 我的企业信息
    else if locals.isAdmin
      li: a(href="#") 上传/注册产品
      li: a(href="#") 添加/管理账户
    else
      li: a(href="/mfrs/register") 注册为生产厂商
    li: a(href="#") 我的账户信息
