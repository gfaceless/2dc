doctype 5
html
  head
    block head
      title= title
      meta(charset="utf-8")
      block css
        link(rel='stylesheet', href='/stylesheets/style.css')
        link(rel='stylesheet', href='/stylesheets/list.css')
        link(rel='stylesheet', href='/stylesheets/category/list.css')
      block js
        script(src="/yui/3.9.0/build/yui/yui-min.js")
  body
    include includes/header
    #main
      include message
      include includes/control
      h1#title= title
      block content
    include includes/footer
    block js-bottom